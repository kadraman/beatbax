{
  "name": "@beatbax/cli",
  "version": "0.1.1",
  "description": "Command-line interface for BeatBax chiptune live-coding language.",
  "private": false,
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "beatbax": "dist/cli.js"
  },
  "keywords": [
    "chiptune",
    "gameboy",
    "music",
    "cli",
    "tracker",
    "live-coding",
    "beatbax"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kadraman/beatbax.git",
    "directory": "packages/cli"
  },
  "license": "MIT",
  "author": "BeatBax Contributors",
  "homepage": "https://github.com/kadraman/beatbax#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc -b",
    "dev": "tsc -b && node dist/cli-dev.js play ../../songs/sample.bax --headless",
    "pretest": "npm run build --if-present",
    "test": "jest --config ./jest.config.cjs --passWithNoTests"
  },
  "dependencies": {
    "@beatbax/engine": "^0.1.1",
    "commander": "^12.1.0",
    "play-sound": "^1.1.6",
    "standardized-audio-context": "^25.3.0"
  },
  "optionalDependencies": {
    "speaker": "^0.5.5"
  }
}
