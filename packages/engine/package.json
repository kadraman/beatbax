{
  "name": "@beatbax/engine",
  "version": "0.2.0",
  "description": "Live-coding language and runtime for retro console chiptunes.",
  "private": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "chiptune",
    "gameboy",
    "music",
    "audio",
    "synthesis",
    "live-coding",
    "apu",
    "tracker",
    "hugetracker",
    "uge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kadraman/beatbax.git",
    "directory": "packages/engine"
  },
  "license": "MIT",
  "author": "BeatBax Contributors",
  "homepage": "https://github.com/kadraman/beatbax#readme",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./parser": "./dist/parser/index.js",
    "./parser/*": "./dist/parser/*.js",
    "./parser/index": "./dist/parser/index.js",
    "./parser/index.js": "./dist/parser/index.js",
    "./export": "./dist/export/index.js",
    "./export/*": "./dist/export/*.js",
    "./import": "./dist/import/index.js",
    "./import/*": "./dist/import/*.js",
    "./song": {
      "browser": "./dist/song/index.browser.js",
      "default": "./dist/song/index.js"
    },
    "./song/resolver": {
      "browser": "./dist/song/resolver.browser.js",
      "default": "./dist/song/resolver.js"
    },
    "./song/importResolver": {
      "browser": "./dist/song/importResolver.browser.js",
      "default": "./dist/song/importResolver.js"
    },
    "./song/*": "./dist/song/*.js",
    "./audio": "./dist/audio/index.js",
    "./audio/*": "./dist/audio/*.js",
    "./chips": "./dist/chips/index.js",
    "./chips/*": "./dist/chips/*.js",
    "./patterns": "./dist/patterns/index.js",
    "./patterns/*": "./dist/patterns/*.js",
    "./sequences": "./dist/sequences/index.js",
    "./sequences/*": "./dist/sequences/*.js",
    "./instruments": "./dist/instruments/index.js",
    "./instruments/*": "./dist/instruments/*.js",
    "./scheduler": "./dist/scheduler/index.js",
    "./scheduler/*": "./dist/scheduler/*.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "generate:peggy": "node ./scripts/generate-peggy-parser.cjs",
    "prebuild": "npm run generate:peggy",
    "build": "tsc -b",
    "dev": "tsc --watch",
    "pretest": "npm run build --if-present",
    "test": "jest --config ./jest.config.cjs --passWithNoTests"
  },
  "dependencies": {
    "standardized-audio-context": "^25.3.0"
  },
  "devDependencies": {
    "peggy": "^4.2.0"
  }
}
