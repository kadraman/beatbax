    "./song/*": "./dist/song/*.js",
    "./audio": "./dist/audio/index.js",
    "./audio/*": "./dist/audio/*.js",
    "./chips": "./dist/chips/index.js",
    "./chips/*": "./dist/chips/*.js",
    "./patterns": "./dist/patterns/index.js",
    "./patterns/*": "./dist/patterns/*.js",
